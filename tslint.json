{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**/*.ts",
      "**/*.css",
      "**/*.svg"
    ]
  },
  "rules": {
    "no-console": false,
    "linebreak-style": [true, "LF"],
    "object-literal-sort-keys": false
  }
}
